{
  "name": "codelinesjs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --mode production",
    "build:analyze": "tsc -b && vite build --mode production --outDir dist-analyze",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:report": "eslint . --output-file eslint-report.json --format json",
    "lint:check": "eslint --print-config src/components/Dashboard/CommunitySection/Ranking/hooks/useRankingData.ts",
    "lint:staged": "eslint --fix $(git diff --name-only --staged | grep -E '\\.(js|jsx|ts|tsx)$')",
    "lint:changed": "eslint --fix $(git diff --name-only | grep -E '\\.(js|jsx|ts|tsx)$')",
    "lint:quiet": "eslint . --quiet",
    "lint:src": "eslint src --max-warnings=0",
    "lint:debug": "eslint --debug src/components/Dashboard/CommunitySection/Ranking/hooks/useRankingData.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "preview": "vite preview",
    "test": "vitest run --config vitest.config.ts src --exclude \"**/*.spec.{ts,tsx}\" --exclude \"e2e/**\" --exclude \"**/*playwright*/**\" --exclude \"**/*e2e*/**\"",
    "test:watch": "vitest --config vitest.config.ts src --exclude \"**/*.spec.{ts,tsx}\" --exclude \"e2e/**\" --exclude \"**/*playwright*/**\" --exclude \"**/*e2e*/**\"",
    "test:unit": "vitest run --config vitest.config.ts src/components --exclude \"**/*.spec.{ts,tsx}\" --exclude \"e2e/**\" --exclude \"**/*playwright*/**\" --exclude \"**/*e2e*/**\"",
    "test:ui": "vitest --ui --config vitest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rimraf dist",
    "prepare": "husky",
    "postinstall": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@monaco-editor/react": "^4.6.0",
    "@react-oauth/google": "^0.12.1",
    "@tanstack/react-query": "^5.62.16",
    "@types/dompurify": "^3.2.0",
    "@types/prettier": "^3.0.0",
    "@types/react-helmet": "^6.1.11",
    "@types/three": "^0.171.0",
    "@xenova/transformers": "^2.17.2",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.4",
    "framer-motion": "^11.15.0",
    "install": "^0.13.0",
    "jwt-decode": "^4.0.0",
    "marked": "^15.0.6",
    "npm": "^11.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.1",
    "react-icon": "^1.0.0",
    "react-icons": "^5.4.0",
    "react-intersection-observer": "^9.15.1",
    "react-router-dom": "^7.1.1",
    "react-select": "^5.10.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-toastify": "^11.0.2",
    "recharts": "^2.15.0",
    "three": "^0.172.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/testing-library__react": "^10.2.0",
    "@types/testing-library__user-event": "^4.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^7.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "globals": "^15.14.0",
    "husky": "^9.0.11",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.2",
    "playwright": "^1.51.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.39.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.0.8"
  },
  "eslintConfig": {},
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,scss}": [
      "prettier --write"
    ]
  }
}
